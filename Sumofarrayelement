package Day1;

public class Sumofarrayelement {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		int a[]= {4,8,7,25,12};
		int sum=0;
		for(int i=0;i<=a.length-1;i++)
		{
			sum=sum+a[i];
		}
		System.out.println("sum of array element are: "+sum);
		
		//using enhanced for loop
//		for(int value:a)
//		{
//			sum=sum+value;
//		}
//		System.out.println("sum of array element are: "+sum);
	}

}
